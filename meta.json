{
  "module_id": "viam-soleng:viam-ros2-integration",
  "visibility": "public",
  "url": "https://github.com/viam-soleng/viam-ros2-integration",
  "description": "Use Viam features on top of your ROS 2 robot, for more information please see: \"https://github.com/viam-soleng/viam-ros2-integration/README.md",
  "models": [
    {
      "api": "rdk:component:base",
      "model": "viam-soleng:ros2:base"
    },
    {
      "api": "rdk:component:camera",
      "model": "viam-soleng:ros2:camera"
    },
    {
      "api": "rdk:component:sensor",
      "model": "viam-soleng:ros2:sensor"
    },
    {
      "api": "rdk:component:movement_sensor",
      "model": "viam-soleng:ros2:imu"
    },
    {
      "api": "rdk:component:camera",
      "model": "viam-soleng:ros2:lidar"
    },
    {
      "api": "viam-soleng:service:ros2_logger",
      "model": "viam-soleng:ros2:logger"
    },
    {
      "api": "viam-soleng:service:action_client",
      "model": "viam-soleng:ros2:action_client"
    },
    {
      "api": "viam-soleng:service:service_client",
      "model": "viam-soleng:ros2:service_client"
    }
  ],
  "entrypoint": "run.sh",
  "build": {
    "build": "make module.tar.gz",
    "path": "module.tar.gz",
    "arch": [
      "linux/arm64",
      "linux/amd64"
    ]
  }
}
